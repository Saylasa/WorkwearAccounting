<Window x:Class="WorkwearAccounting.SearchNormWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorkwearAccounting"
        mc:Ignorable="d"
        Title="SearchNormWindow" Height="161" Width="334">
    <Grid>
        <Label x:Name="lblName" Content="Должность*:" HorizontalAlignment="Left" Margin="10,22,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnCancel" Content="Отмена" HorizontalAlignment="Left" Margin="183,90,0,0" VerticalAlignment="Top" Width="131" Height="31" Click="CloseForm"/>
        <Button x:Name="btnSearch" Content="Поиск" HorizontalAlignment="Left" Margin="10,90,0,0" VerticalAlignment="Top" Width="131" Height="31" Click="btnSearch_Click" />
        <ComboBox x:Name="cbEmplPosition" HorizontalAlignment="Left" Margin="94,26,0,0" VerticalAlignment="Top" Width="220" ItemsSource="{Binding}" DisplayMemberPath="Name" />

    </Grid>
</Window>
